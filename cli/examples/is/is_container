#!/bin/bash
#┌──────────────────────┐
#│ Загружаем библиотеку │
#└──────────────────────┘
. ~/lib

#┌───────────────┐
#│ Версия образа │
#└───────────────┘
IMAGE_NAME='c9start'

# Контейнер не найден
    if ! is_container "ancestor=$IMAGE_NAME"; then
        echo 'Контейнеры не найдены!'
        
# Контейнер найден
    else
        echo 'Список контейнеров:'
        echo "$GLOBAL_ID"
    fi
    