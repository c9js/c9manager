#┌───────────────┐
#│ Версия образа │
#└───────────────┘
IMAGE_NAME='c9start'

# Образы не найдены
    if ! is_image "$IMAGE_NAME"; then
        echo 'Образы не найдены!'
        
# Образы найдены
    else
        echo 'Список образов:'
        echo "$GLOBAL_ID"
    fi
    