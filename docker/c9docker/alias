alias c9="$PATH_WORKSPACE/scripts/c9start/entrypoint.sh"
alias build='c9 build'
alias manager='c9 manager'

alias c9docker='build c9docker'
alias c9start='build c9start'
alias c9open='build c9open'
alias c9vue='build c9vue'

alias remove='build --remove c9docker'

alias deploy="$HOME/scripts/deploy/deploy.sh"

alias amend="$HOME/scripts/amend/amend.sh"
