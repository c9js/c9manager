alias c9="/$WORKSPACE/docker/c9.sh"

alias c9docker="c9 build c9docker"
alias c9start="c9 build c9start"
alias c9open="c9 build c9open"

alias build="c9 build c9docker"
alias remove="c9 remove c9docker"
alias start="c9 start c9docker"
alias stop="c9 stop c9docker"

alias d="/$WORKSPACE/scripts/deploy/deploy.sh"
alias deploy="d"
