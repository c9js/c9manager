alias c9="$PATH_WORKSPACE/scripts/c9start/entrypoint.sh"
alias b="c9 build"
alias build="b"

alias c9docker="b c9docker"
alias c9start="b c9start"
alias c9open="b c9open"

alias remove="b --remove c9docker"

alias d="$HOME/scripts/deploy/deploy.sh"
alias deploy="d"

alias a="$HOME/scripts/amend/amend.sh"
alias amend="a"
